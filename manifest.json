{
  "manifest_version": 2,

  "name": "Dark mode extension",
  "description": "This extension will analyze a page using Dark mode",
  "version": "1.2",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+D",
        "mac": "MacCtrl+Shift+D"
      },
      "description": "Dark mode on"
    }
  },
  "permissions": [
    "activeTab"
  ]
}